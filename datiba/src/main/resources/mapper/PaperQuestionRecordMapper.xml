<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datiba.mapper.PaperQuestionRecordMapper">

    <resultMap id="BaseResultMap" type="com.datiba.domain.PaperQuestionRecord">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="paperId" column="paper_id" jdbcType="INTEGER"/>
            <result property="questionId" column="question_id" jdbcType="INTEGER"/>
            <result property="createdTime" column="created_time" jdbcType="TIMESTAMP"/>
            <result property="updatedTime" column="updated_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,paper_id,question_id,
        created_time,updated_time
    </sql>
    <insert id="saveRecord" parameterType="java.util.List">
        INSERT INTO paper_question_record (paper_id, question_id)
        VALUES
        <foreach collection="list" item="pair" separator=",">
            (#{pair.paperId}, #{pair.questionId})
        </foreach>
    </insert>
    <select id="searchQIdByPId" resultType="java.lang.Integer">
        select question_id
        from paper_question_record
        where paper_id = #{paperId}
        order by question_id
    </select>
</mapper>
