<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datiba.mapper.SystemQuestionBankMapper">

    <select id="selectMenu" resultType="com.datiba.domain.dto.BankMenuDto">
        select id,name
        from system_question_bank
        where parent_id = 0;
    </select>
    <select id="listDto" resultType="com.datiba.domain.dto.SysQuestionBankDto">
        select id,name,parent_id
        from system_question_bank
        where parent_id != 0;
    </select>
</mapper>
